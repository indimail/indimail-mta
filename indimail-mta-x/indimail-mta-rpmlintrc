addFilter(".* non-standard-gid")
addFilter(".* permissions-file-digest-unauthorized /etc/permissions.d/indimail-mta.*")
addFilter(".* file-parent-ownership-mismatch .*")
addFilter(".* unstripped-binary-or-object .*")
addFilter(".* filelist-forbidden-fhs23 /var/indimail")
addFilter(".* non-standard-dir-in-var .*")
addFilter(".* non-standard-uid")
addFilter(".* potential-bashisms .*")
setBadness('update-alternatives-post-call-missing', 0)
setBadness('permissions-unauthorized-file', 0)
setBadness('permissions-file-setuid-bit', 0)
setBadness('permissions-directory-setuid-bit', 0)
