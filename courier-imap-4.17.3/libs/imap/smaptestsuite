(
echo "open INBOX"
n=0
while test $n -lt 64
do
	m=$n
	if test "$m" = ""
	then
		m=""
	fi

	echo "STORE ${m}3-${m}5 ${m}7 ${m}9 +FLAGS=DELETED"
	n=`expr $n + 1`
done
echo "LOGOUT"
) | env IMAP_BROKENUIDV=1 MAILDIR=confmdtest PROTOCOL=SMAP1 ./imapd | sed "$SED"

