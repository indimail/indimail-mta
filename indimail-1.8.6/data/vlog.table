#!/var/indimail/sbin/vtable
vlog
(id bigint primary key auto_increment,
user char(40),
passwd char(28),
domain char(67),
logon char(32),
remoteip char(18),
message varchar(254),
timestamp timestamp,
error int,
index user_idx (user),
index domain_idx (domain),
index remoteip_idx (remoteip),
index error_idx (error),
index message_idx (message))
