addFilter(".* non-standard-uid")
addFilter(".* non-standard-gid")
addFilter(".* non-standard-dir-in-var .*")
addFilter(".* file-in-unusual-dir .*")
addFilter(".* hardening-no-relro .*")
addFilter(".* hardening-no-fortify-functions .*")
addFilter(".* debian-changelog-line-too-long .*")
addFilter(".* extended-description-line-too-long")
addFilter("W: suse-filelist-forbidden-fhs23 /var/indimail is not allowed in FHS 2.3")
addFilter("W: permissions-file-setuid-bit")
addFilter("W: permissions-directory-setuid-bit .* is packaged with setuid/setgid bits .*")
addFilter("W: permissions-missing-verifyscript missing %verify_permissions -e /var/indimail/bin/.*")
addFilter("W: file-not-in-%lang .*")
addFilter("W: source-or-patch-not-bzipped .*")
addFilter("W: source-or-patch-not-compressed bz2 .*")
setBadness('permissions-unauthorized-file', 0)
setBadness('permissions-file-setuid-bit', 0)
setBadness('non-position-independent-executable', 0)
