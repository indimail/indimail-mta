+static void rr_random(struct rfc1035_rr *p, unsigned n)
+{
+	unsigned i;
+
+	for (i=0; i<n; i++)
+	{
+		size_t j=rand() % n;
+
+		{
+			struct rfc1035_rr buf=p[j];
+
+			p[j]=p[i];
+			p[i]=buf;
+		}
+	}
