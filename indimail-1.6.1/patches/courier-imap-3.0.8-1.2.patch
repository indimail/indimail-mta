diff -Naur Courier-imap-3.0.8/unicode/unicode_wcwidth.c courier-imap-3.0.8/unicode/unicode_wcwidth.c
--- Courier-imap-3.0.8/unicode/unicode_wcwidth.c	1970-01-01 05:30:00.000000000 +0530
+++ courier-imap-3.0.8/unicode/unicode_wcwidth.c	2009-05-31 14:11:47.000000000 +0530
@@ -0,0 +1,29 @@
+#include "unicode_config.h"
+#include "unicode.h"
+
+#include "eastasianwidth.h"
+
+#include <stdlib.h>
+
+int unicode_wcwidth(unicode_char c)
+{
+	size_t b=0;
+	size_t e=sizeof(unicode_wcwidth_tab)/sizeof(unicode_wcwidth_tab[0]);
+
+	while (b < e)
+	{
+		size_t n=b + (e-b)/2;
+
+		if (c >= unicode_wcwidth_tab[n][0])
+		{
+			if (c <= unicode_wcwidth_tab[n][1])
+				return 2;
+			b=n+1;
+		}
+		else
+		{
+			e=n;
+		}
+	}
+	return 1;
+}
