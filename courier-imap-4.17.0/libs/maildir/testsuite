rm -rf testmd
./maildirmake testmd
./maildirmake -q 3C testmd
ls | ./deliverquota testmd
echo $? | cut -c1
ls | ./deliverquota testmd
echo $? | cut -c1
ls | ./deliverquota testmd
echo $? | cut -c1
ls | ./deliverquota testmd
echo $? | cut -c1

rm -rf md
./maildirmake md
echo ""
./maildirkw -L md | sort
./maildirkw md one Label0 Label1
echo ""
./maildirkw -L md | sort
./maildirkw md two Label0 Label1
echo ""
./maildirkw -L md | sort
./maildirkw md two Label1 Label2
echo ""
./maildirkw -L md | sort
./maildirkw md three Label2 Label3
echo ""
./maildirkw -L md | sort
./maildirkw md three Label3
./maildirkw -a md three Label4
echo ""
./maildirkw -L md | sort
./maildirkw -r md one Label0
echo ""
./maildirkw -L md | sort
./maildirkw -r md one Label1
echo ""
./maildirkw -L md | sort
rm -rf md testmd
